{
  "name": "typitap",
  "version": "0.1.0",
  "description": "Typing game",
  "scripts": {
    "eslint": "eslint *.js client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "kjarosinski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:iKonrad/Typitap.git"
  },
  "dependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.23.1",
    "eslint": "^3.15.0",
    "expose-loader": "^0.7.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "jquery": "^3.2.1",
    "postcss-functions": "^2.1.0",
    "prop-types": "^15.5.8",
    "proxy-middleware": "^0.15.0",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-notification-system-redux": "^1.1.2",
    "react-redux": "^4.4.8",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-form": "^6.6.1",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "timeago-react": "^1.1.0",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "whatwg-fetch": "^2.0.3",
    "when": "^3.7.8"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "dev": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ]
  },
  "devDependencies": {
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.5.1",
    "sass-loader": "^4.1.1",
    "svgo-loader": "^1.2.1",
    "url-loader": "^0.5.8"
  }
}
